<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jcstool.repository.mapper.UserMapper">

    <select id="queryAll" resultType="com.jcstool.pojo.User" >
        SELECT
            id,
            username `name`,
            `password`,
            created
        FROM
            `user`
        limit 5
    </select>
</mapper>